{
  "name": "svelte5-typescript-bun-template",
  "version": "0.0.0",
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.31",
    "@julr/unocss-preset-forms": "^0.1.0",
    "@julr/unocss-preset-heropatterns": "^1.0.2",
    "@playwright/test": "^1.43.0",
    "@sveltejs/adapter-auto": "^3.2.0",
    "@sveltejs/kit": "^2.5.5",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@types/bun": "latest",
    "@unocss/svelte-scoped": "^0.59.0",
    "commit-and-tag-version": "^12.2.0",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "svelte": "^5.0.0-next.96",
    "svelte-check": "^3.6.9",
    "tslib": "^2.6.2",
    "typescript": "^5.4.4",
    "unocss": "^0.59.0",
    "unocss-preset-theme": "^0.12.0",
    "vite": "^5.2.8"
  },
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "bun test --watch",
    "e2e": "playwright test",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "deploy": "bunx gh-pages -d build --dotfiles",
    "release": "commit-and-tag-version",
    "postrelease": "git push --follow-tags origin main"
  },
  "type": "module"
}
